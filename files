apiVersion: serving.kserve.io/v1beta1
kind: InferenceService
metadata:
  name: test-model
  annotations:
    serving.kserve.io/storage-initializer-sourceuri: "s3://your-bucket/model"
    # Override the storage initializer image
    serving.kserve.io/storage-initializer-image: "your-registry/storage-initializer-with-cert:latest"
spec:
  predictor:
    sklearn:
      storageUri: "s3://your-bucket/model"
